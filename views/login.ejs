<div class="page-wrap">
   <div class="container">
      <div class="page-header">
         <h1>Login</h1>
      </div>
   </div>
   <div class="container">
      <div class="panel-body">
         <div class="panel-heading">
            {% if (locals.message) { %}
               <p class="text-info lead">{%= message %}</p>
            {% }else{ %}
               <p class="text-info lead">debug: no messages</p>
            {% } %}
            {% if (locals.error) { %}
               <p class="lead">Error: <span class="text-danger">{%= error %}</span></p>
            {% }else{ %}
               <p class="text-success lead">debug: no errors</p>
            {% } %}
         </div>

         <form class="" action="/auth/facebook" method="post">
            <button type="submit" class="btn btn-default" style="border:none;">
               <!-- <button onclick="displayError()" class="btn btn-default" style="border:none;"> -->
               <img src="images/fbLoginBtn.png" class="img-responsive" />
            </button>
         </form>
      </div>
   </div>
</div>
